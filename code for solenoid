    delay(450);

     digitalWrite(solenoidPins[2][3], LOW);
    digitalWrite(solenoidPins[1][3], LOW);
    delay(15);
    digitalWrite(solenoidPins[2][3], HIGH);
    digitalWrite(solenoidPins[1][3], HIGH);
    

    delay(250);

    digitalWrite(solenoidPins[0][3], LOW);
